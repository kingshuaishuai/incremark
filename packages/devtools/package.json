{
  "name": "@incremark/devtools",
  "version": "0.3.6",
  "license": "MIT",
  "description": "Incremark DevTools - Framework-agnostic debugging SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && pnpm build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "@incremark/core": "workspace:*"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "json-formatter-js": "^2.5.23"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "svelte": "^5.46.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "keywords": [
    "incremark",
    "devtools",
    "debugging",
    "markdown",
    "developer-tools",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kingshuaishuai/incremark.git",
    "directory": "packages/devtools"
  },
  "homepage": "https://www.incremark.com/"
}
