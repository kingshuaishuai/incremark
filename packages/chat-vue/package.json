  "peerDependencies": {
    "vue": "^3.5.26"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@iconify/vue": "^4.3.0",
    "@incremark/chat-core": "workspace:*",
    "@incremark/shared": "workspace:*",
    "@incremark/theme": "workspace:*",
    "@incremark/vue": "workspace:*",
    "@tiptap/extension-document": "^3.15.3",
    "@tiptap/extension-paragraph": "^3.15.3",
    "@tiptap/extension-text": "^3.15.3",
    "@tiptap/extensions": "^3.15.3",
    "@tiptap/markdown": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/vue-3": "^3.15.3",
    "@vueuse/core": "^14.1.0"
