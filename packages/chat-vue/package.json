{
  "name": "@incremark/chat-vue",
  "version": "0.4.0-alpha.1",
  "description": "Vue 3 implementation of Incremark Chat-UI with incremental rendering support.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
