    "@storybook/vue3": "^10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue-macros/volar": "^3.1.2",
    "esbuild-plugin-vue3": "^0.5.1",
    "remark-gfm": "^4.0.1",
    "storybook": "^10.1.11",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  },
  "keywords": [
    "ai",
    "chat",
